{
  "type": "object",
  "$schema": "http://json-schema.org/draft-03/schema",
  "id": "http://jsonschema.net",
  "required": true,
  "properties": {
    "apiVersion": {
      "type": "string",
      "required": true,
      "enum": [ "v1alpha1" ]
    },
    "kind": {
      "type": "string",
      "required": true,
      "enum": [ "User" ]
    },
    "metadata": {
      "type": "object",
      "required": true,
      "properties": {
        "id": {
          "type": string,
          "required": true
        },
        "name": {
          "type": "string",
          "required": true
        },
        "email": {
          "type": "string",
          "required": true
        },
        "access_token": {
          "type": "string",
          "required": true
        },
        "admin": {
          "type": "boolean",
          "required": true
        },
        "VOs": {
          "type": "array",
          "required": true,
          "items": {
            "type": "string",
            "required": true
          }
        }
      }
    }
  }
}