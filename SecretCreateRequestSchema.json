{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "http://jsonschema.net",
  "required": true,
  "properties": {
    "apiVersion": {
      "type": "string",
      "enum": [ "v1alpha1" ]
    },
    "metadata": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "vo": {
          "type": "string"
        },
        "cluster": {
          "type": "string"
        }
      },
      "required": ["name","vo","cluster"]
    },
    "contents": {
      "type": object
    }
  },
  "required": ["apiVersion","metadata","contents"]
}