{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "http://jsonschema.net",
  "properties": {
    "apiVersion": {
      "type": "string",
      "enum": [ "v1alpha1" ]
    },
    "kind": {
      "type": "string",
      "enum": [ "Secret" ]
    },
    "metadata": {
      "type": "object",
      "properties": {
        "id": {
          "type": string
        },
        "name": {
          "type": "string"
        },
        "vo": {
          "type": "string"
        },
        "cluster": {
          "type": "string"
        },
        "created": {
          "type": "string"
        }
      },
      "required": ["id","name","application","vo","cluster","created"]
    },
    "contents": {
      "type": "object"
    }
  },
  "required": ["apiVersion","kind","metadata","contents"]
}