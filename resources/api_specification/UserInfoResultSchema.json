{
  "type": "object",
  "$schema": "http://json-schema.org/draft-07/schema",
  "id": "http://jsonschema.net",
  "required": true,
  "properties": {
    "apiVersion": {
      "type": "string",
      "enum": [ "v1alpha1" ]
    },
    "kind": {
      "type": "string",
      "enum": [ "User" ]
    },
    "metadata": {
      "type": "object",
      "properties": {
        "id": {
          "type": string
        },
        "name": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "institution": {
          "type": "string"
        },
        "access_token": {
          "type": "string"
        },
        "admin": {
          "type": "boolean"
        },
        "VOs": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "required": ["id","name","email","phone","institution","access_token","admin","VOs"]
    }
  },
  "required": ["apiVersion","kind","metadata"]
}